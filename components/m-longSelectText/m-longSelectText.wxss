/* components/m-longSelectText/m-longSelectText.wxss */
.selectContainer{
  position: fixed;
  top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 500px;
  background-color: rgba(255, 235, 205, 0) ;
  z-index: 4;
}

.select {
  position: absolute;
  z-index: 6;
}

.btn {
  border-radius: 0;
  width: 200rpx;
  background-color: white;
  font-size: 35rpx;
  height: 80rpx;
  box-shadow: 1px 1px 2px #88888850;
}

.btn::after {
  border-radius: 0;
}
